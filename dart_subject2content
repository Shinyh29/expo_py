


url = "https://opendart.fss.or.kr/api/list.json?crtfc_key=" + api_key + "&bgn_de=20200605&end_de=20200605&corp_cls=K&page_no=1&page_count=1000"

#* corp_cls	법인구분	STRING(1)	default : N	,  
#----select (법인구분) 
# Y(유가),
# K(코스닥), 
# N(코넥스), 
# E(기타)
#※ 없으면 전체조회, 복수조건 불가


